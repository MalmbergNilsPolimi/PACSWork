# Doxyfile 1.9.1

# This file describes the settings to be used by the documentation system
# doxygen (www.doxygen.org) for a project.

#---------------------------------------------------------------------------

# Encoding specification
DOXYFILE_ENCODING      = UTF-8

# Name of the project
PROJECT_NAME           = "Challenge1"

# A line to describe the project
PROJECT_BRIEF          = "A gradient method for the minimization of a multivariate function developped by Nils Malmberg."

# Output language
OUTPUT_LANGUAGE        = English

# Specify the working directory
OUTPUT_DIRECTORY       = ./doc

# Set to NO if you don't want to generate documentation for undocumented members
EXTRACT_ALL            = YES

# Set to YES to include the full path in the file names in the file documentation
FULL_PATH_NAMES        = YES

# Set to YES to enable preprocessing
ENABLE_PREPROCESSING   = YES

# Set to YES to include source files as examples in the documentation
SOURCE_BROWSER         = YES

# The RECURSIVE tag can be used to turn specify whether or not subdirectories should be searched for input files as well.
RECURSIVE              = YES

# The EXCLUDE tag can be used to specify files and/or directories that should be excluded from the input.
EXCLUDE                = 

# The INPUT tag can be used to specify the files and/or directories that contain documented source files.
INPUT                  = ./src ./include

# The FILE_PATTERNS tag can be used to specify file patterns that should be excluded from the input.
FILE_PATTERNS          = *.cpp *.hpp

# The GENERATE_LATEX tag can be used to specify whether or not latex should be generated.
GENERATE_LATEX         = YES